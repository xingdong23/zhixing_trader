{
  "backtest_name": "GridBNB-ETH/USDT动态网格策略回测",
  "description": "ETH/USDT 动态网格策略 2个月历史数据回测 (2024年9-10月)",
  
  "data": {
    "source": "backtest/data/ETHUSDT-5m-2months.csv",
    "timeframe": "5m",
    "resample_from": "5m"
  },
  
  "strategy": {
    "name": "gridbnb_usdt",
    "config_file": "../app/config/gridbnb_usdt.json"
  },
  
  "backtest_settings": {
    "initial_capital": 1000.0,
    "window_size": 200,
    "taker_fee_rate": 0.001,
    "maker_fee_rate": 0.001,
    "slippage_rate": 0.0001
  },
  
  "output": {
    "result_file": "results/gridbnb_eth_backtest_{timestamp}.json",
    "report_file": "results/gridbnb_eth_report_{timestamp}.txt",
    "save_trades": true,
    "save_equity_curve": true
  }
}
