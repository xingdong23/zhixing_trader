{
  "backtest_name": "高频策略回测 - 多文件示例",
  "description": "演示如何使用多个历史数据文件进行回测",
  
  "data": {
    "source": "data/ETH-USDT-*.csv",
    "timeframe": "5m",
    "resample_from": "1m",
    "comment": "支持多种格式: 单文件/多文件列表/目录/通配符"
  },
  
  "strategy": {
    "name": "high_frequency",
    "config_file": "../app/config/high_frequency.json"
  },
  
  "backtest_settings": {
    "initial_capital": 300.0,
    "window_size": 200,
    "commission_rate": 0.0004,
    "slippage_rate": 0.0001
  },
  
  "output": {
    "result_file": "results/backtest_result_{timestamp}.json",
    "report_file": "results/backtest_report_{timestamp}.txt",
    "save_trades": true,
    "save_equity_curve": true
  }
}
