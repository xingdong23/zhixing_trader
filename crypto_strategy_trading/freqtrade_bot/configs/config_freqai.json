{
    "freqai": {
        "enabled": true,
        "purge_old_models": 2,
        "train_period_days": 15,
        "backtest_period_days": 7,
        "live_retrain_hours": 1,
        "expiration_hours": 2,
        "identifier": "volatility_predictor_5m",
        "feature_parameters": {
            "include_timeframes": [
                "5m",
                "15m",
                "1h"
            ],
            "include_corr_pairlist": [
                "BTC/USDT:USDT",
                "ETH/USDT:USDT"
            ],
            "indicator_periods_candles": [
                10,
                20,
                50
            ],
            "label_period_candles": 4,
            "DI_threshold": 1,
            "weight_factor": 0.9,
            "principal_component_analysis": false
        },
        "data_split_parameters": {
            "test_size": 0.2,
            "random_state": 42,
            "shuffle": false
        },
        "model_training_parameters": {
            "n_estimators": 200,
            "learning_rate": 0.05,
            "max_depth": 6
        }
    }
}